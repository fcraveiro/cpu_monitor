<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows</TargetFramework> <!-- Define o framework .NET 8.0 para Windows -->
    <Nullable>enable</Nullable> <!-- Habilita a verificação de nulabilidade -->
    <UseWindowsForms>true</UseWindowsForms> <!-- Habilita o uso de Windows Forms -->
    <ImplicitUsings>enable</ImplicitUsings> <!-- Habilita usings implícitos -->
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="LibreHardwareMonitorLib" Version="0.9.2" />
  </ItemGroup>

</Project>
